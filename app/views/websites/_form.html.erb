<%= form_for @website do |f| %>
    <%= f.label :url %>
    <%= f.text_field :url %> <br />
    <%= f.label :description %>
    <%= f.text_area :description %> <br />

    <%= f.label :search_place %>
    <%= f.text_field :search_place %> <br />

    <%= f.label :content %>
    <%= f.text_field :content %> <br />

    <%= f.submit %>
<% end %>